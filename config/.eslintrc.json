 {
   "env": {
     "browser": true,
     "commonjs": true,
     "es2021": true,
     "jquery": true,
     "mocha": true
   },
   "extends": "eslint:recommended",
   "parserOptions": {
     "ecmaVersion": 12,
     "sourceType": "module"
   },
   "rules": {
     "quotes": [
       "error",
       "double"
     ],
     "semi": [
       "error",
       "always"
     ]
   }
 }